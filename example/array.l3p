Program Main 
var i : numeric
var arr : array[10] of numeric
var arr2 : array[3] of numeric
begin

  i := 0
  while i < 10 do
    arr[i] := i * 2
    i := i + 1
  endwhile

  i := 0
  while i < 5 do
    printf('%d\n', arr[i + 1])
    i := i + 1
  endwhile

  i := 0
  arr2[i + 0] := 100
  arr2[i + 1] := 200
  arr2[i + 2] := 300

  i := 0
  while i < 3 do
    printf('%d\n', arr2[i])
    i := i + 1
  endwhile

end

